{
  "expo": {
    "name": "Coolmind",
    "slug": "coolmind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/system/icon.png",
    "splash": {
      "image": "./app/assets/system/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/system/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/system/favicon.png"
    },
    "description": "Coolmind is a self-guided mental health app with quick exercises, each one regarding different themes, to help you get calm, connected, and balanced. So whenever you start feeling unpleasant thoughts or emotions, reach out to your app and get yourself a cool mind :)",
    "githubUrl": "https://github.com/andre-rd-rodrigues/coolmind"
  }
}
